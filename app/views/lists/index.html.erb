      <th>Item15</th>
      <th>Item16</th>
      <th>Item17</th>
      <th>Item18</th>
      <th>Item19</th>
      <th>Item20</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.title %></td>
        <td><%= list.item1 %></td>
        <td><%= list.item2 %></td>
        <td><%= list.item3 %></td>
        <td><%= list.item4 %></td>
        <td><%= list.item5 %></td>
        <td><%= list.item6 %></td>
        <td><%= list.item7 %></td>
        <td><%= list.item8 %></td>
        <td><%= list.item9 %></td>
        <td><%= list.item10 %></td>
