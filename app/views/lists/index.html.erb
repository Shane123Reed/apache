        <td><%= list.item13 %></td>
        <td><%= list.item14 %></td>
        <td><%= list.item15 %></td>
        <td><%= list.item16 %></td>
        <td><%= list.item17 %></td>
        <td><%= list.item18 %></td>
        <td><%= list.item19 %></td>
        <td><%= list.item20 %></td>
        <td><%= list.user %></td>
        <td><%= link_to 'Show', list %></td>
        <td><%= link_to 'Edit', edit_list_path(list) %></td>
        <td><%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

